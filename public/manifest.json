{
  "name": "Twip chatbox test tool",
  "description": "Twip Chatbox의 테스트 채팅을 생성하는 크롬확장입니다.",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": ["tabs", "scripting", "alarms"],
  "host_permissions": ["https://twip.kr/*"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icon/icon16.png",
    "32": "icon/icon32.png",
    "48": "icon/icon48.png",
    "128": "icon/icon128.png"
  }
}
